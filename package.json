{
  "name": "react-content-manager",
  "version": "3.1.1",
  "description": "enable editable components in your react app",
  "keywords": [
    "react",
    "content-manager"
  ],
  "homepage": "https://github.com/pavelee/react-content-manager#readme",
  "bugs": {
    "url": "https://github.com/pavelee/react-content-manager/issues"
  },
  "author": "PaweÅ‚ Ciosek",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavelee/react-content-manager.git"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "test": "jest --config jestconfig.json",
    "prepare": "husky",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "watch": "nodemon --watch './src/**/*' -e js,json,jsx,ts,tsx --exec 'npm run build'",
    "lint": "eslint ./src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "check-exports": "attw --pack .",
    "ci": "npm run build && npm run test && npm run check-format && npm run check-exports",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {
    "antd": "^5.23.1"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.3",
    "@changesets/cli": "^2.27.11",
    "@eslint/js": "^9.18.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.7",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "react-dom": "^19.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
